pipeline{
	agent any
	stages{
		stage('scm checkout')
		{
				steps
				{
					git branch: 'master', url: 'https://github.com/Manisha-jharbade/maven-project'
				}
		}
		stage('Sonar and Maven')
		{
			steps{
				withSonarQubeEnv('sonar') {
							withMaven(jdk: 'localjdk-1.8', maven:'maven')
							{							
								sh 'clean mvn install sonar:sonar'
							}	
					}
			}
			
		}		
	}
}



